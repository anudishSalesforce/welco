    <description>Update due to formula limitations for Mosaic Work Order Report</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>IF(
AND(
ISBLANK(TEXT([Work_Order__c].CompleteandClose__c)),
ISBLANK([Work_Order__c].Close_Date__c) ),
&quot;Repair Not Complete&quot;,

IF(
AND(
NOT(ISBLANK([Work_Order__c].Arrival_Date__c )),
NOT(ISBLANK([Work_Order__c].Close_Date__c )),
[Work_Order__c].Close_Date__c &lt;= [Work_Order__c].Expected_Completion_Date__c),
&quot;Y&quot;,

IF(
AND(
NOT(ISBLANK([Work_Order__c].Arrival_Date__c )),
NOT(ISBLANK([Work_Order__c].Close_Date__c)),
[Work_Order__c].Close_Date__c &gt; [Work_Order__c].Expected_Completion_Date__c),
&quot;N&quot;,
null
)
)
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_2_myRule_1_A3_2142302762</name>
        <dataType>String</dataType>
        <expression>IF(
AND(
ISBLANK(TEXT({!myVariable_current.CompleteandClose__c})),
ISBLANK({!myVariable_current.Close_Date__c}) ),
&quot;Repair Not Complete&quot;,

IF(
AND(
NOT(ISBLANK({!myVariable_current.Arrival_Date__c} )),
NOT(ISBLANK({!myVariable_current.Close_Date__c} )),
{!myVariable_current.Close_Date__c} &lt;= {!myVariable_current.Expected_Completion_Date__c}),
&quot;Y&quot;,

IF(
AND(
NOT(ISBLANK({!myVariable_current.Arrival_Date__c} )),
NOT(ISBLANK({!myVariable_current.Close_Date__c})),
{!myVariable_current.Close_Date__c} &gt; {!myVariable_current.Expected_Completion_Date__c}),
&quot;N&quot;,
null
)
)
)</expression>
    </formulas>
    <formulas>
