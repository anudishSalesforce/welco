            <name>originalFormula</name>
            <value>
                <stringValue>IF(
AND(
NOT(ISBLANK([Work_Order__c].Arrival_Date__c )),
NOT(ISBLANK( [Work_Order__c].X4_5_Estimate_Actual_Completion__c )),
NOT(ISBLANK(  [Work_Order__c].X52AActualPOApproval__c  ))
),
