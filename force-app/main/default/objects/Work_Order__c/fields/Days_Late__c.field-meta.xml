<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Days_Late__c</fullName>
    <description>If the quote was not provided within Mosaic&apos;s target KPI of 5, then calculate for the difference in days to determine # days late for Mosaic&apos;s RS Template</description>
    <externalId>false</externalId>
    <formula>IF(
 Provided_Within_Target__c = &quot;N&quot;, 
 VALUE(Days_to_Quote__c) - 5,
null)</formula>
    <label>Days Late</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
