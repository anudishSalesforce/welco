<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Estimated_Cost_per_Student__c</fullName>
    <description>Calculates estimated cost per student</description>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(TotalCost__c), null, TotalCost__c / (Total_Enrolled__c + Total_Waitlist_InReview__c))</formula>
    <inlineHelpText>Estimated cost per student based on total cost and total students enrolled and waitlisted/in review</inlineHelpText>
    <label>Estimated Cost per Student</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
